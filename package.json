{
  "name": "zabo-server",
  "version": "0.0.0",
  "description": "This repository is powered by SPARCS KAIST",
  "main": "index.js",
  "private": true,
  "scripts": {
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "start": "node index",
    "dev": "yarn env:dev nodemon index --watch src",
    "prod": "yarn env:prod nodemon index",
    "seed": "node tools/seed"
  },
  "husky": {
    "hooks": {
      "pre-commit": "$npm_execpath lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "aws-sdk": "^2.485.0",
    "axios": ">=0.18.1",
    "chalk": "^2.4.2",
    "connect-redis": "^3.4.1",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "dotenv-expand": "^5.1.0",
    "dotenv-safe": "^6.1.0",
    "express": "~4.16.0",
    "express-async-handler": "^1.1.4",
    "express-session": "^1.16.1",
    "graphql-redis-subscriptions": "^2.1.1",
    "graphql-yoga": "^1.18.3",
    "helmet": "^3.16.0",
    "http-errors": "~1.6.2",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "migrate-mongoose": "^4.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "node-cron": "^2.0.3",
    "s3-image-size": "^0.1.3",
    "saslprep": "^1.0.3",
    "signale": "^1.4.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0",
    "zabo-utils": "^0.0.6"
  },
  "devDependencies": {
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "figlet": "^1.2.4",
    "husky": "^4.0.6",
    "inquirer": "^7.0.3",
    "lint-staged": "^9.5.0",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "arrow-parens": 0,
      "import/no-unresolved": 0,
      "import/prefer-default-export": 0,
      "no-nested-ternary": "off",
      "no-unused-vars": "warn",
      "no-unused-expressions": "warn",
      "space-before-function-paren": [
        "warn",
        "always"
      ],
      "no-spaced-func": "off",
      "func-call-spacing": [
        "warn",
        "always"
      ],
      "no-underscore-dangle": "off",
      "no-mixed-spaces-and-tabs": "error",
      "no-shadow": "warn",
      "max-len": [
        "warn",
        150
      ],
      "prefer-const": "warn",
      "consistent-return": [
        "warn",
        {
          "treatUndefinedAsUnspecified": true
        }
      ],
      "no-await-in-loop": "warn"
    },
    "env": {
      "browser": false,
      "node": true
    }
  }
}
