{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "rules": {
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0,
    "no-nested-ternary": "off",
    "no-unused-vars": "warn",
    "no-unused-expressions": "warn",
    "no-spaced-func": "off",
    "no-underscore-dangle": "off",
    "no-shadow": "warn",
    "prefer-const": "warn",
    "consistent-return": [
      "warn",
      {
        "treatUndefinedAsUnspecified": true
      }
    ],
    "no-await-in-loop": "warn"
  },
  "env": {
    "browser": false,
    "node": true
  }
}
